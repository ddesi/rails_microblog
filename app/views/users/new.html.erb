<h1>Join us!</h1>
<h1>Sign up to start your incredible adventure</h1><br>
<%= form_for User.new do |f| %>
	<%= f.text_field :fname, placeholder: "Choose a first name" %><br><br>
	<%= f.text_field :lname, placeholder: "Choose a last name" %><br><br>
	<%= f.email_field :email, placeholder: "Choose an email" %><br><br>
	<%= f.password_field :password, placeholder: "Choose a password" %><br><br>
	<%= f.submit :submit %>
<% end %>